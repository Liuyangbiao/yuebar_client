<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_main"
    android:orientation="vertical" >

    <com.fullteem.yueba.widget.TopTitleView
        android:id="@+id/top_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.fullteem.yueba.widget.OverScrollView
        android:id="@+id/os"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="6dp" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/tv_nonPayment"
                style="@style/tv_blacktitle_style"
                android:layout_marginTop="5dp"
                android:background="@drawable/date_content_light2dark_selector"
                android:drawableRight="@drawable/down_arrow_icon"
                android:paddingBottom="9dp"
                android:paddingLeft="9dp"
                android:paddingRight="8dp"
                android:paddingTop="9dp"
                android:text="@string/non_payment"
                android:textSize="15sp" />

            <com.fullteem.yueba.widget.ExpandListView
                android:id="@+id/lv_nonPayment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/content_bottom_bg"
                android:cacheColorHint="@color/transparent"
                android:divider="@drawable/bg_nearby_date_dividerline"
                android:dividerHeight="0.9dp"
                android:listSelector="@color/transparent"
                android:padding="0dp"
                android:scrollbars="none"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tv_nonConsume"
                style="@style/tv_blacktitle_style"
                android:layout_marginTop="5dp"
                android:background="@drawable/date_content_light2dark_selector"
                android:drawableRight="@drawable/down_arrow_icon"
                android:paddingBottom="9dp"
                android:paddingLeft="9dp"
                android:paddingRight="8dp"
                android:paddingTop="9dp"
                android:text="@string/non_consume"
                android:textSize="15sp" />

            <com.fullteem.yueba.widget.ExpandListView
                android:id="@+id/lv_nonConsume"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/content_bottom_bg"
                android:cacheColorHint="@color/transparent"
                android:divider="@drawable/bg_nearby_date_dividerline"
                android:dividerHeight="0.9dp"
                android:listSelector="@color/transparent"
                android:scrollbars="none"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tv_consumed"
                style="@style/tv_blacktitle_style"
                android:layout_marginTop="5dp"
                android:background="@drawable/date_content_light2dark_selector"
                android:drawableRight="@drawable/down_arrow_icon"
                android:paddingBottom="9dp"
                android:paddingLeft="9dp"
                android:paddingRight="8dp"
                android:paddingTop="9dp"
                android:text="@string/consumed"
                android:textSize="15sp" />

            <com.fullteem.yueba.widget.ExpandListView
                android:id="@+id/lv_consumed"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/content_bottom_bg"
                android:cacheColorHint="@color/transparent"
                android:divider="@drawable/bg_nearby_date_dividerline"
                android:dividerHeight="0.9dp"
                android:listSelector="@color/transparent"
                android:scrollbars="none"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tv_backOrder"
                style="@style/tv_blacktitle_style"
                android:layout_marginTop="5dp"
                android:background="@drawable/date_content_light2dark_selector"
                android:drawableRight="@drawable/down_arrow_icon"
                android:paddingBottom="9dp"
                android:paddingLeft="9dp"
                android:paddingRight="8dp"
                android:paddingTop="9dp"
                android:text="@string/back_order"
                android:textSize="15sp" />

            <com.fullteem.yueba.widget.ExpandListView
                android:id="@+id/lv_backOrder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/content_bottom_bg"
                android:cacheColorHint="@color/transparent"
                android:divider="@drawable/bg_nearby_date_dividerline"
                android:dividerHeight="0.9dp"
                android:listSelector="@color/transparent"
                android:scrollbars="none"
                android:visibility="gone" />
        </LinearLayout>
    </com.fullteem.yueba.widget.OverScrollView>

</LinearLayout>