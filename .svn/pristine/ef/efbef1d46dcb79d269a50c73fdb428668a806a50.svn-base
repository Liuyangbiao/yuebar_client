<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="MyDialogStyle">
        <item name="android:windowBackground">@drawable/layout_corners_round_bg</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="MySureDialog" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/layout_corners_round_bg</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="devide_line_eee">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1.5dp</item>
        <item name="android:background">#eee</item>
    </style>

    <style name="chat_text_date_style">
        <item name="android:padding">2dp</item>
        <item name="android:paddingTop">2dip</item>
        <item name="android:paddingBottom">2dip</item>
        <item name="android:paddingLeft">12dip</item>
        <item name="android:paddingRight">12dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">@color/transparent</item>
    </style>

    <style name="chat_text_name_style">
        <item name="android:textColor">#818181</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="chat_content_date_style">
        <item name="android:minHeight">50dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">15sp</item>
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:background">#bfbfbf</item>
    </style>

    <style name="AnimFade2" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/slide_in_from_right</item>
        <item name="android:activityOpenExitAnimation">@anim/slide_out_to_left</item>
        <item name="android:activityCloseExitAnimation">@anim/slide_out_to_right</item>
        <item name="android:activityCloseEnterAnimation">@anim/slide_in_from_left</item>
    </style>

    <style name="horizontal_slide" parent="android:Theme.NoTitleBar">
        <item name="android:windowAnimationStyle">@style/AnimFade2</item>
    </style>

    <!-- 幸运相遇弹出 -->
    <style name="TransparentActivity">
        <item name="android:windowBackground">@color/transparent_background</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/TransparentAnimation</item>
    </style>
    <!-- 幸运相遇弹出动画 -->
    <style name="TransparentAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/popwindow_show</item>
    </style>

    <style name="tv_loginmain_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#232323</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:drawablePadding">7dp</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="edt_loginmain_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">14sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:padding">3dp</item>
        <item name="android:maxLength">20</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <!-- for title -->
    <style name="toptitle_left" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#faffffff</item>
        <item name="android:textSize">16sp</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="toptitle_right" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#faffffff</item>
        <item name="android:textSize">16sp</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="toptitle_center" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#faffffff</item>
        <item name="android:textSize">22sp</item>
        <item name="android:singleLine">true</item>
    </style>

    <!-- 附近模块 -->
    <style name="btn_nearby_style" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- 黑色TextView style -->
    <style name="tv_blacktitle_style" parent="@android:style/Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ff202020</item>
        <item name="android:textSize">13sp</item>
        ></style>

    <style name="tv_datestate_style" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingTop">7dp</item>
        <item name="android:paddingBottom">7dp</item>
        <item name="android:textColor">#ff222222</item>
        <item name="android:textSize">22sp</item>
        <item name="android:background">@drawable/bg_nearby_data_state</item>
    </style>

    <!-- 登陆checkbox -->
    <style name="LoginCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/login_checkbox_selector</item>
        <item name="android:paddingLeft">25.0dip</item>
        <item name="android:maxHeight">5.0dip</item>
        <item name="android:textSize">@dimen/size_10</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- 注册编辑框 -->
    <style name="edt_register_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">14sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:maxLength">20</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <!-- 注册页面文本 -->
    <style name="tv_register_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#232323</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:drawablePadding">7dp</item>
        <item name="android:singleLine">true</item>
    </style>

    <!-- 性别选择，男，checkbox -->
    <style name="SexBoyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/sex_boy_checkbox_selector</item>
        <item name="android:paddingLeft">25.0dip</item>
        <item name="android:maxHeight">5.0dip</item>
        <item name="android:textSize">@dimen/size_10</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- 性别选择，女，checkbox -->
    <style name="SexGirlCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/sex_girl_checkbox_selector</item>
        <item name="android:paddingLeft">25.0dip</item>
        <item name="android:maxHeight">5.0dip</item>
        <item name="android:textSize">@dimen/size_10</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- 附近几个页面的灰色说明字样 -->
    <style name="GrayWords">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:textSize">@dimen/size_12</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- 评价栏 -->
    <style name="articlelist_item_ratingbar_style" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating_drawable_selector</item>
        <item name="android:numStars">5</item>
        <item name="android:minHeight">@dimen/size_16</item>
        <item name="android:maxHeight">@dimen/size_17</item>
    </style>

    <!-- popwindow出现动画 -->
    <style name="PopupAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/popwindow_show</item>
        <item name="android:windowExitAnimation">@anim/popwindow_dismiss</item>
    </style>

    <!-- listpopupwindow 出现动画 -->
    <style name="listpopupwindow_style">
        <item name="android:windowEnterAnimation">@anim/top2bottom_in</item>
        <item name="android:windowExitAnimation">@anim/top2bottom_out</item>
    </style>

    <!-- 白色小按钮 -->
    <style name="btn_small_white" parent="android:Widget.Button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/btn_white_selector</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:paddingLeft">6dp</item>
        <item name="android:paddingRight">6dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">10sp</item>
    </style>

    <!-- 红色小按钮 -->
    <style name="btn_small_red" parent="android:Widget.Button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/btn_red_selector</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:paddingLeft">6dp</item>
        <item name="android:paddingRight">6dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="btn_option_item" parent="android:Widget.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/content_light2dark_selector</item>
        <item name="android:drawableRight">@drawable/right_arrow_white_icon</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:padding">10dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="perssonal_tv_black_style" parent="@style/GrayWords">
        <item name="android:paddingBottom">6dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:textColor">#b9000000</item>
    </style>

    <!-- 个人信息页面bind的style -->
    <style name="perssonal_tv_black_style_bind" parent="@style/GrayWords">
        <item name="android:paddingBottom">6dp</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:textColor">#b9000000</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <!-- 红色通用title -->
    <style name="red_common_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/girl_red</item>
        <item name="android:textSize">17sp</item>
    </style>

    <!-- 按钮 蓝色-白色，大多蓝色按钮都是该style -->
    <style name="btn_bgBule2White_textWhite2Black" parent="android:Widget.Button">
        <item name="android:background">@drawable/btn_9blue2white_selector</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">1dp</item>
        <item name="android:paddingLeft">6dp</item>
        <item name="android:paddingRight">6dp</item>
        <item name="android:paddingTop">1dp</item>
        <item name="android:textColor">@drawable/text_color_white2black_selector</item>
        <item name="android:textSize">9sp</item>
    </style>

    <!-- 按钮 红-白，大多红色按钮都是该style -->
    <style name="btn_bgred2White_textWhite2Black" parent="btn_bgBule2White_textWhite2Black">
        <item name="android:background">@drawable/btn_9red2white_selector</item>
    </style>

    <!-- 性别文本框style -->
    <style name="tvGenderSryle" parent="android:Widget.TextView">
        <item name="android:background">@drawable/tv_gender_selector</item>
        <item name="android:drawableLeft">@drawable/sex_girl</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">18dp</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/size_10</item>
    </style>

    <!-- 性别文本框style(男) -->
    <style name="tvGenderStyleBoy" parent="android:Widget.TextView">
        <item name="android:background">@drawable/barfriend_boy_bg</item>
        <item name="android:drawableLeft">@drawable/sex_boy</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">18dp</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/size_10</item>
    </style>

    <style name="line_gray">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/size_1</item>
        <item name="android:background">@drawable/line_gray</item>
    </style>

    <style name="base_listview_style" parent="@android:style/Widget.ListView">
        <item name="android:scrollbarThumbVertical">@drawable/base_list_scrollbar_handle</item>
        <item name="android:fadingEdgeLength">0.0dip</item>
        <item name="android:alwaysDrawnWithCache">true</item>
        <item name="android:listSelector">@drawable/base_list_selector</item>
        <item name="android:drawSelectorOnTop">false</item>
        <item name="android:cacheColorHint">@null</item>
        <item name="android:divider">@null</item>
        <item name="android:dividerHeight">0.0dip</item>
    </style>

    <style name="title_update_progressbar_style">
        <item name="android:maxWidth">45.0dip</item>
        <item name="android:maxHeight">45.0dip</item>
        <item name="android:indeterminateDrawable">@anim/title_update_anim</item>
        <item name="android:minWidth">45.0dip</item>
        <item name="android:minHeight">45.0dip</item>
    </style>

    <!-- spinner -->
    <style name="spinner_style">
        <item name="android:background">@drawable/spinner_selector</item>
        <item name="android:paddingLeft">5dip</item>
    </style>

    <!-- 重新发送消息对话框 -->
    <style name="MsgDialogStyle">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <!-- 性别左边性别右边年龄Style -->
    <style name="genderAndAge_girl" parent="android:Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">18dp</item>
        <item name="android:background">@drawable/tv_gender_selector</item>
        <item name="android:drawableLeft">@drawable/sex_girl</item>
        <item name="android:drawablePadding">1dp</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:paddingTop">1dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/size_12</item>
    </style>

    <!-- 查看图片页面 gfwstyle -->
    <style name="picture_watch">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <!-- 4.0以上弹窗样式 -->

    <style name="window_high_style" parent="android:Theme.Holo.Light">
        <item name="android:windowAnimationStyle">@style/AnimFade2</item>
    </style>

</resources>