<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_loginmain_back"
    android:orientation="vertical" >

    <com.fullteem.yueba.widget.TopTitleView
        android:id="@+id/toptile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true" />

    <LinearLayout
        android:id="@+id/ll_second_title"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_below="@+id/toptile"
        android:layout_marginBottom="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="22dp"
        android:layout_marginTop="22dp"
        android:orientation="horizontal" >

        <include layout="@layout/secend_dynamic_title_bar" />
    </LinearLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/vp_fragment_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/ll_second_title" />
</RelativeLayout>